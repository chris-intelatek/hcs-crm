<div class="col-md-4 col-md-offset-4">
	<div class="panel panel-gray">
	  <div class="panel-heading">
	  	<h4>Editing: &nbsp; <%= @prospect.organization %></h4>
	  </div>
	  <div class="prospect-show">
  		<%= simple_form_for @prospect, url: hcs_lead_status_update_path, method: :post do |f| %>
			<div class="row">
				<div class="col-md-8 col-md-offset-2">
					<%= f.input :hcs_lead_status, collection: true, required: true, collection: [
																									'Submitted',
																									'Pending Review',
																									'Approved',
																									'Declined'
																									], label: "Lead Status" %></div>
			</div>
		</div>
		<div class="panel-footer center">
			<%= link_to "Back", entrust_path, class: "btn btn-default" %>
      <%= f.button :submit, class:"btn btn-slate" %>
    <% end %>
  	</div>
	</div>
</div>